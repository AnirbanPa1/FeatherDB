{
  "configurations": [
    {
      "name": "Linux",
      "compileCommands": "${workspaceFolder}/compile_commands.json",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/deps/c_std",
        "${workspaceFolder}/deps/c_std/time"
      ],
      "defines": [
        "_GNU_SOURCE"
      ],
      "cStandard": "c17",
      "intelliSenseMode": "linux-gcc-x64"
    },
    {
      "name": "Windows",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/deps/c_std",
        "${workspaceFolder}/deps/c_std/time",
        "${workspaceFolder}/deps/c_std/dependency/include"
      ],
      "defines": [
        "_WIN32",
        "_CRT_SECURE_NO_WARNINGS"
      ],
      "cStandard": "c17",
      "intelliSenseMode": "msvc-x64"
    }
  ],
  "version": 4
}
