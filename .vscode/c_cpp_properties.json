{
  "configurations": [
    {
      "name": "windows-gcc-x86",
      "compileCommands": "${workspaceFolder}/compile_commands.json",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "_GNU_SOURCE"
      ],
      "cStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x86",
      "cppStandard": "${default}",
      "compilerPath": "C:/MinGW/bin/gcc.exe"
    },
    {
      "name": "Windows",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/deps/c_std",
        "${workspaceFolder}/deps/c_std/time",
        "${workspaceFolder}/deps/c_std/dependency/include"
      ],
      "defines": [
        "_WIN32",
        "_CRT_SECURE_NO_WARNINGS"
      ],
      "cStandard": "c17",
      "intelliSenseMode": "msvc-x64"
    }
  ],
  "version": 4
}